udessa, mutta koska mallinnukset ovat usein jonkun tietyn yrityksen tilauksia ongelmien ratkaisuiksi, niit\"{a} ei julkaista. K\"{a}sitellyist\"{a} l\"{a}hteist\"{a} saatiin kuitenkin hyv\"{a} kuva eri sovelluksista ja mahdollisuuksista. Systeemidynaamisista malleista voi olla todella suurta hy\"{o}ty\"{a}, kun niit\"{a} k\"{a}ytet\"{a}\"{a}n ongelmien ratkaisussa ja p\"{a}\"{a}t\"{o}ksenteon tukena.},author = {Sorasalmi, Tomi},file = {:C$\backslash$:/Users/jtsalmi/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Sorasalmi - 2009 - Systeemidynamiikka mets\"{a}teollisuudessa.pdf:pdf},keywords = {mallintaminen,mets\"{a}teollisuus,paperiteollisuus,systeemidynamiikka},pages = {5 + 27},school = {Aalto University},title = {{Systeemidynamiikka mets\"{a}teollisuudessa}},year = {2009}}@phdthesis{Mulari2011a,abstract = {T\"{a}m\"{a}n kandidaatinty\"{o}n tavoitteena on esitell\"{a} palveluliiketoiminnassa k\"{a}ytett\"{a}vi\"{a} systeemidynaamisia malleja eri n\"{a}k\"{o}kulmista. Ty\"{o}ss\"{a} tarkastellaan palvelun ja palveluliiketoiminnan k\"{a}sitett\"{a} sek\"{a} esitell\"{a}\"{a}n systeemidynaamista ajattelutapaa, ilmi\"{o}it\"{a} ja mallintamisen vaiheita. Palveluliiketoimintaa k\"{a}sitell\"{a}\"{a}n yrityksen elinkaaren, kysynn\"{a}n ennustamisen, laadun ja hinnan tasapainon, innovaatioiden ja riskien sek\"{a} ty\"{o}voiman ja osaamisen johtamisen teemojen kautta. Kunkin teeman yhteydess\"{a} k\"{a}yd\"{a}\"{a}n l\"{a}pi kirjallisuudessa esitettyj\"{a} systeemidynaamisia malleja, joita on kehitetty teemaan liittyvien ilmi\"{o}iden mallintamiseksi. Ty\"{o}ss\"{a} pohditaan esiteltyjen mallien soveltuvuutta k\"{a}yt\"{a}nn\"{o}n tilanteisiin sek\"{a} pohditaan esitettyj\"{a} palvelun ja systeemidynamiikan k\"{a}sitteit\"{a}. Palvelun k\"{a}sitteen ymm\"{a}rt\"{a}miseksi ty\"{o}ss\"{a} k\"{a}ytet\"{a}\"{a}n monessa yhteydess\"{a} niin sanottua IHIP-m\"{a}\"{a}ritelm\"{a}\"{a}, jonka mukaan palvelu on aineeton, erilaatuinen, erottamaton ja pilaantuva. Systeemidynamiikka ymm\"{a}rret\"{a}\"{a}n er\"{a}\"{a}n\"{a} simulointikeinona, jolla voidaan mallintaa poikkitieteellisi\"{a} ilmi\"{o}it\"{a}, mutta joka on pohjimmiltaan matemaattinen l\"{a}hestymistapa mallinnettaviin ongelmiin. Ty\"{o}n keskeisen\"{a} tuloksena esitet\"{a}\"{a}n, ett\"{a} systeemidynamiikka soveltuu erityisesti sellaisten kvantitatiivisten ilmi\"{o}iden mallintamiseen, jotka ovat helposti mitattavissa. Toisaalta systeemidynamiikkaa on k\"{a}ytetty viimeisten vuosikymmenten aikana kattavasti n\"{a}iden ilmi\"{o}iden mallintamiseen, ja sen soveltuvuuden arvioiminen kvalitatiivisiin ilmi\"{o}ihin on nyt ajankohtaista. Palveluliiketoimintaan liittyy runsaasti n\"{a}it\"{a} kvalitatiivisia ilmi\"{o}it\"{a}, joiden mittaaminen on haastavaa. Systeemidynamiikan roolin esitettiin laajenevan tulevaisuuden yhteiskunnissa, koska tuotteiden ja palveluiden toimitusketjut monimutkaistuvat. Laajojen ketjujen hallinta onnistuu kokonaisuuksia tarkastelemalla ja v\"{a}ltt\"{a}m\"{a}ll\"{a} ketjun yksitt\"{a}isen osapuolen tuloksellisuuden maksimointia.},author = {Mulari, Mikko},file = {:C$\backslash$:/Users/jtsalmi/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mulari - 2011 - Palveluliiketoiminnan systeemidynaamiset mallit.pdf:pdf},keywords = {mallinnus,palveluliiketoiminta,palvelut,simulointi,systeemidynamiikka},pages = {30},school = {Aalto University},title = {{Palveluliiketoiminnan systeemidynaamiset mallit}},year = {2011}}@phdthesis{Nurmi2012a,author = {Nurmi, Marko},file = {:C$\backslash$:/Users/jtsalmi/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Nurmi - 2012 - Sairauksiin liittyv\"{a}t systeemidynamiikan sovellukset.pdf:pdf},keywords = {mallinnus,sairaus,simulointi,systeemidynamiikka,terveydenhoito},pages = {3+34},school = {Aalto University},title = {{Sairauksiin liittyv\"{a}t systeemidynamiikan sovellukset}},year = {2012}}@phdthesis{Rantio2011a,abstract = {T\"{a}m\"{a}n kandidaatinty\"{o}n tavoitteena on tarkastella 